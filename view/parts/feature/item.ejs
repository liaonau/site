<%_
    var width
    if (!width)
        width = 6
_%>
<div class="col-lg-<%= width -%>">
    <div class="d-flex wow fadeInLeft" data-wow-delay="<%= delay -%>s">
        <div class="flex-shrink-0 btn-lg-square rounded-circle bg-primary">
            <i class="fa fa-check text-white"></i>
        </div>
        <div class="ms-4">
            <h4><%- h %></h4>
            <span><%- text %></span>
        </div>
    </div>
</div>
